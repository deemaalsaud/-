  <section>
    <div class="w-layout-blockcontainer container-40 w-container">
      <div class="w-embed w-script">
        <!DOCTYPE html>
        <html lang="en">
        <head>
          <meta charset="UTF-8">
          <meta name="viewport" content="width=device-width, initial-scale=1.0">
          <title>Discrete Mathematics Game</title>
          <link rel="stylesheet" href="styles.css">
        </head>
        <body>
          <h2>Discrete Mathematics Game</h2>
          <p>Welcome to the Discrete Mathematics Game! Demonstrate your understanding of set operations by completing the objectives.</p>
          <div class="container">
            <div class="set" id="setA" ondrop="drop(event)" ondragover="allowDrop(event)">
              <h3>Set A</h3>
            </div>
            <div class="set" id="setB" ondrop="drop(event)" ondragover="allowDrop(event)">
              <h3>Set B</h3>
            </div>
          </div>
          <div class="elements">
            <div class="element" id="element1" draggable="true" ondragstart="drag(event)">1</div>
            <div class="element" id="element2" draggable="true" ondragstart="drag(event)">2</div>
            <div class="element" id="element3" draggable="true" ondragstart="drag(event)">3</div>
            <div class="element" id="element4" draggable="true" ondragstart="drag(event)">4</div>
            <div class="element" id="element5" draggable="true" ondragstart="drag(event)">5</div>
          </div>
          <div class="objectives">
            <h3>Objectives:</h3>
            <ul id="objectives-list">
              <li>Place all even numbers in Set A and all odd numbers in Set B.</li>
              <li>Create a set that represents the union of Set A and Set B.</li>
              <li>Perform the intersection of Set A and Set B.</li>
            </ul>
          </div>
          <div id="game-result"></div>
          <script>
    var objectives = [
        "Place all even numbers in Set A and all odd numbers in Set B.",
        "Create a set that represents the union of Set A and Set B.",
        "Perform the intersection of Set A and Set B."
    ];
    var currentObjectiveIndex = 0;
    var totalObjectives = objectives.length;
    var currentObjective = objectives[currentObjectiveIndex];
    var gameResultDiv = document.getElementById("game-result");
    // Display initial objective
    displayObjective(currentObjective);
    function displayObjective(objective) {
        gameResultDiv.innerHTML = "<strong>Objective:</strong> " + objective;
    }
    function allowDrop(ev) {
        ev.preventDefault();
    }
    function drag(ev) {
        ev.dataTransfer.setData("text", ev.target.id);
    }
    function drop(ev) {
        ev.preventDefault();
        var data = ev.dataTransfer.getData("text");
        ev.target.appendChild(document.getElementById(data));
        // Check if the current objective is completed
        checkObjectiveCompletion();
    }
    function checkObjectiveCompletion() {
        var elementsInSetA = document.getElementById("setA").childElementCount;
        var elementsInSetB = document.getElementById("setB").childElementCount;
        switch (currentObjectiveIndex) {
            case 0: // Objective 1: Place all even numbers in Set A and all odd numbers in Set B
                if (elementsInSetA === 2 && elementsInSetB === 3) {
                    // Move to the next objective
                    moveToNextObjective();
                }
                break;
            case 1: // Objective 2: Create a set that represents the union of Set A and Set B
                if (elementsInSetA + elementsInSetB === 5) {
                    // Move to the next objective
                    moveToNextObjective();
                }
                break;
            case 2: // Objective 3: Perform the intersection of Set A and Set B
                if (elementsInSetA === 2 && elementsInSetB === 2) {
                    // All objectives completed
                    gameResultDiv.innerHTML = "Congratulations! You have completed all objectives!";
                }
                break;
        }
    }
    function moveToNextObjective() {
        if (currentObjectiveIndex < totalObjectives - 1) {
            currentObjectiveIndex++;
            currentObjective = objectives[currentObjectiveIndex];
            displayObjective(currentObjective);
        }
    }
</script>
        </body>
        </html>
      </div>
